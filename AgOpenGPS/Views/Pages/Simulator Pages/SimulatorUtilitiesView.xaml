<Page
    x:Class="AgOpenGPS.Views.Pages.Simulator_Pages.SimulatorUtilitiesView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AgOpenGPS.Views.Pages.Simulator_Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="SimulatorUtilitiesView"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Border MinHeight="500">
        <Border.Resources>
            <Style TargetType="{x:Type Border}">
                <Setter Property="Background">
                    <Setter.Value>
                        <SolidColorBrush Color="{DynamicResource ApplicationBackgroundColor}" />
                    </Setter.Value>
                </Setter>

                <Setter Property="Margin" Value="20,10" />
                <Setter Property="CornerRadius" Value="10" />
                <Setter Property="BorderBrush" Value="#DAAC0C" />
                <Setter Property="BorderThickness" Value="1" />
            </Style>
            <Style TargetType="TextBlock">
                <Setter Property="FontSize" Value="20" />
            </Style>
        </Border.Resources>
        <TabControl>
            <TabItem Header="Wizards (Steer Wizards)">
                <Grid Margin="10,5" ShowGridLines="False">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="auto" />
                    </Grid.RowDefinitions>
                    <Grid Margin="0,5" ShowGridLines="False">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>
                        <ui:Button
                            Name="btnRestartWizard"
                            Margin="10,0"
                            Appearance="Danger"
                            Click="btnRestartWizard_Click"
                            Content="Restart Wizard"
                            Foreground="White" />
                        <Grid Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="1*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="10,0"
                                VerticalAlignment="Center"
                                FontSize="14"
                                Text="Progress" />
                            <ProgressBar
                                x:Name="progressbar"
                                Grid.Column="1"
                                Margin="0,0"
                                Maximum="25"
                                Minimum="0" />
                        </Grid>
                        <ui:Button
                            Name="btnStartWizard"
                            Grid.Column="2"
                            Margin="10,0"
                            Appearance="Success"
                            Click="btnStartWizard_Click"
                            Content="Start Wizard"
                            Foreground="White" />
                    </Grid>

                    <Border Grid.Row="1" Margin="0,0,0,0">
                        <Grid Margin="5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <ContentControl>
                                    <ContentControl.Resources>
                                        <DataTemplate x:Key="1">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Load Default" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="1*" />
                                                    </Grid.RowDefinitions>
                                                    <TextBlock VerticalAlignment="Center" Text="Highly Recommended" />
                                                    <ui:Button
                                                        Grid.Row="1"
                                                        MinHeight="45"
                                                        Margin="0,5"
                                                        HorizontalAlignment="Stretch"
                                                        Appearance="Primary">
                                                        <Grid>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*" />
                                                                <ColumnDefinition Width="auto" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock VerticalAlignment="Center" Text="Load Default" />
                                                            <Image
                                                                Grid.Column="1"
                                                                Height="50"
                                                                Margin="5,0,0,0"
                                                                Source="../../../../Assets/OK64.png" />
                                                        </Grid>
                                                    </ui:Button>
                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="2">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="WheelBase" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto" />
                                                        <ColumnDefinition Width="1*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid Margin="0,60,0,0" VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="1*" />
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Text="Set WheelBase" />
                                                        <ui:NumberBox Grid.Row="1" MinWidth="150" />

                                                    </Grid>
                                                    <Image
                                                        Grid.Column="1"
                                                        Height="200"
                                                        Margin="10,0,0,0"
                                                        Source="../../../../Assets/RadiusWheelBase.png" />

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="3">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Wheel Track" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto" />
                                                        <ColumnDefinition Width="auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Image
                                                        Grid.Column="0"
                                                        Height="200"
                                                        Margin="10,0,0,0"
                                                        Source="../../../../Assets/RadiusWheelBase.png" />
                                                    <Grid
                                                        Grid.Column="1"
                                                        Margin="0,-10,0,0"
                                                        VerticalAlignment="Top">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="1*" />
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Text="Set Track Width" />
                                                        <ui:NumberBox Grid.Row="1" MinWidth="150" />

                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="4">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Antenna Position" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Image
                                                        Grid.Row="0"
                                                        Height="200"
                                                        Margin="10,0,0,0"
                                                        Source="../../../../Assets/vehiclePageTractor.png" />
                                                    <Grid
                                                        Grid.Row="1"
                                                        Margin="0,-30,0,0"
                                                        VerticalAlignment="Top">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="1*" />
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Top"
                                                            Text="Set Distance Antenna Is From Pivote Axle" />
                                                        <ui:NumberBox Grid.Row="1" MinWidth="150" />

                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="5">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Antenna Height" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto" />
                                                        <ColumnDefinition Width="auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Image
                                                        Grid.Column="0"
                                                        Height="200"
                                                        Margin="10,0,0,0"
                                                        Source="../../../../Assets/vehiclePageTractor.png" />
                                                    <Grid
                                                        Grid.Column="1"
                                                        Margin="0,0,0,0"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="1*" />
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Text="Set Antenna Height" />
                                                        <ui:NumberBox Grid.Row="1" MinWidth="150" />

                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="6">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Antenna Offset" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="0"
                                                        Margin="0,-30,0,0"
                                                        VerticalAlignment="Top">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="1*" />
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Top"
                                                            Text="Antenna Offset" />
                                                        <ui:NumberBox Grid.Row="1" MinWidth="150" />

                                                    </Grid>
                                                    <Image
                                                        Grid.Row="1"
                                                        Height="200"
                                                        Margin="10,0,0,0"
                                                        Source="../../../../Assets/vehiclePageTractor.png" />

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="7">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Steer Switch" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid
                                                            Grid.Row="0"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />
                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="Steer Enable Mode" />
                                                            <ComboBox Grid.Row="1" MinWidth="150" />

                                                            <TextBlock
                                                                Grid.Row="2"
                                                                Margin="0,10"
                                                                VerticalAlignment="Center">
                                                                <Run Text="Button - Push Release, On." />
                                                                <LineBreak />
                                                                <Run Text="            - Push Release, Off." />
                                                            </TextBlock>
                                                            <TextBlock
                                                                Grid.Row="3"
                                                                Margin="0,10"
                                                                VerticalAlignment="Center">
                                                                <Run Text="Switch -While Pushed is On." />
                                                                <LineBreak />
                                                                <Run Text="            -  Released is Off." />
                                                            </TextBlock>

                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="8">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="A/D Convertor" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid
                                                            Grid.Row="0"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />

                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="A/D Convertor Connection?" />
                                                            <ComboBox Grid.Row="1" MinWidth="150" />



                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="9">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Moter Driver Board" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid
                                                            Grid.Row="0"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />

                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="What Style of Moter Driver?" />
                                                            <ComboBox Grid.Row="1" MinWidth="150" />



                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="10">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Driver Control Room" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid
                                                            Grid.Row="0"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />

                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="Invert Hydraulic Enable Relay?" />
                                                            <ToggleButton
                                                                Grid.Row="1"
                                                                Margin="5"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Stretch">
                                                                <ToggleButton.Resources>
                                                                    <Color x:Key="SystemAccentColorSecondary">LightGreen</Color>
                                                                </ToggleButton.Resources>
                                                                <Grid>
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="1*" />
                                                                        <RowDefinition Height="auto" />
                                                                    </Grid.RowDefinitions>
                                                                    <Image Height="60" Source="../../../../Assets/Config/ConSt_InvertRelay.png" />
                                                                    <TextBlock
                                                                        Grid.Row="1"
                                                                        Margin="0,3"
                                                                        HorizontalAlignment="Center"
                                                                        VerticalAlignment="Center"
                                                                        FontSize="14"
                                                                        Text="Invert Relay" />
                                                                </Grid>
                                                            </ToggleButton>



                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="11">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Danfoss Valve" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid
                                                            Grid.Row="0"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />

                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="Do you have use a Danfoss Valve?" />
                                                            <ToggleButton
                                                                Grid.Row="1"
                                                                Margin="5"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Stretch">
                                                                <ToggleButton.Resources>
                                                                    <Color x:Key="SystemAccentColorSecondary">LightGreen</Color>
                                                                </ToggleButton.Resources>
                                                                <Grid>
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="1*" />
                                                                        <RowDefinition Height="auto" />
                                                                    </Grid.RowDefinitions>
                                                                    <Image Height="60" Source="../../../../Assets/Config/ConST_Danfoss.png" />

                                                                </Grid>
                                                            </ToggleButton>



                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="12">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Roll Function" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="auto" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Top"
                                                            TextAlignment="Center">
                                                            <Run Text="Put vehicale leaning strongly to the right." />
                                                            <LineBreak />
                                                            <Run Text="Roll to the right must be POSITIVE" />
                                                        </TextBlock>
                                                        <Grid
                                                            Grid.Row="1"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />
                                                                <RowDefinition Height="auto" />

                                                            </Grid.RowDefinitions>
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top"
                                                                Text="If not Selected Invert Roll?" />
                                                            <ToggleButton
                                                                Grid.Row="1"
                                                                Margin="5"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Stretch">
                                                                <ToggleButton.Resources>
                                                                    <Color x:Key="SystemAccentColorSecondary">LightGreen</Color>
                                                                </ToggleButton.Resources>
                                                                <Image Height="80" Source="../../../../Assets/Config/ConDa_InvertRoll.png" />
                                                            </ToggleButton>
                                                            <TextBlock
                                                                Grid.Row="2"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center">
                                                                <Run
                                                                    FontSize="16"
                                                                    FontWeight="Bold"
                                                                    Text="Roll: " />
                                                                <Run
                                                                    FontSize="14"
                                                                    FontWeight="SemiBold"
                                                                    Text="0" />
                                                            </TextBlock>



                                                        </Grid>
                                                        <Grid
                                                            Grid.RowSpan="3"
                                                            Grid.Column="1"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center">
                                                            <Image Source="../../../../Assets/Config/ConD_RollHelper.png" />
                                                        </Grid>


                                                    </Grid>

                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                        <DataTemplate x:Key="13">
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontWeight="Bold"
                                                    Text="Roll Zero" />
                                                <Grid
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid
                                                        Grid.Row="1"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                            <RowDefinition Height="auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="auto" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Top"
                                                            TextAlignment="Center">
                                                            <Run Text="Park on level surface and touch  " />
                                                            <Run FontWeight="Bold" Text="Zero Roll" />
                                                        </TextBlock>
                                                        <Grid
                                                            Grid.Row="1"
                                                            Margin="0,0,0,0"
                                                            VerticalAlignment="Top">
